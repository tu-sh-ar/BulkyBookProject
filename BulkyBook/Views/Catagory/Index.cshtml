
@model IEnumerable<Catagory> 
@{
    ViewData["Title"] = "Index";
}

<div class="container d-flex p-0">
    <h1 class="col-6">Catagory   </h1>
    <a class="btn btn-warning col-6" asp-controller="Catagory" asp-action="Create" >New Catagory +</a>
</div>
<table class = "table table-bordered table-striped  bg-light">
    <thead >
        <th>Id</th>
        <th>Catagory</th>
        <th>Order</th>
        <th>Date</th>
    </thead>

    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.Id
                </td>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.DisplayOrder
                </td>
                <td>
                    @obj.CreatedDateTime
                </td>
            </tr>
           
            
        }
        
    </tbody>
</table>

