
@model Catagory
@{
    ViewData["Title"] = "Create Catagory";
}

<form method="post">
    <div class="border p-3 mt-4">
        
  <div class="row mb-3">
    <label for="inputEmail3" class="col-sm-2 col-form-label" asp-for="Name">Name</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputEmail3" asp-for="Name">
    </div>
    <span asp-validation-for="Name" class="text-danger"></span>
  </div>
  <div class="row mb-3">
    <label for="inputPassword3" class="col-sm-2 col-form-label" asp-for="DisplayOrder">DisplayOrder</label>
    <div class="col-sm-10">
      <input type="password" class="form-control" id="inputPassword3" asp-for="DisplayOrder">
    </div>
    <span asp-validation-for="DisplayOrder" class="text-danger"></span>
  </div>
 
  <button type="submit" class="btn btn-success">Submit</button>
  <button type="submit" class="btn btn-primary" asp-action="Index">Back</button>

    </div>
</form>


